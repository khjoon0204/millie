    <stylesheets>
        <URL value="@ratingview.css" />
    </stylesheets>
   <children>
      <TextArea fx:id="reviewTextArea" focusTraversable="false" layoutX="36.0" layoutY="142.0" prefHeight="164.0" prefWidth="333.0">
