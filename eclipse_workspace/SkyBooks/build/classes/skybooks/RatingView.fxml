            <Font name="Malgun Gothic" size="14.0" />
         </font>
      </TextArea>
      <Rating fx:id="starRating" layoutX="36.0" layoutY="55.0" prefHeight="30.0" prefWidth="268.0" rating="5.0" />
      <Label layoutX="36.0" layoutY="26.0" text="점수">
